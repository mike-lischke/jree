{
    "name": "jree",
    "version": "1.0.3",
    "description": "Java Runtime Environment Emulation",
    "homepage": "https://soft-gems.net/",
    "repository": {
        "type": "git",
        "url": "https://github.com/mike-lischke/jree"
    },
    "keywords": [
        "Java",
        "JRE",
        "JDK",
        "Java Runtime Environment"
    ],
    "type": "commonjs",
    "main": "./output/src/index.js",
    "author": "Mike Lischke",
    "license": "MIT",
    "dependencies": {
        "immutable": "4.2.4",
        "minimatch": "7.1.3",
        "printf": "0.6.1",
        "unicode-properties": "1.4.1"
    },
    "devDependencies": {
        "@types/jest": "29.4.0",
        "@types/node": "18.11.18",
        "@types/unicode-properties": "1.3.0",
        "@typescript-eslint/eslint-plugin": "5.50.0",
        "@typescript-eslint/parser": "5.50.0",
        "eslint": "8.33.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsdoc": "39.8.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "jest": "29.4.3",
        "ts-jest": "29.0.5",
        "typescript": "4.9.5"
    },
    "scripts": {
        "prepublishOnly": "npm run build && npm run test",
        "build": "tsc",
        "test": "jest --testMatch [ \"**/test/**/*.spec.ts\" ] --no-coverage --watchAll=false --max-worker=1",
        "test-ci": "jest --testMatch [ \"**/test/**/*.spec.ts\" ] --no-coverage --watchAll=false --silent",
        "test-coverage": "jest  --testMatch [ \"**/test/**/*.spec.ts\" ] --coverage --silent",
        "lint": "eslint \"./src/**/*.ts\""
    },
    "jest": {
        "testEnvironment": "node",
        "preset": "ts-jest",
        "verbose": false,
        "testPathIgnorePatterns": [
            "node_modules"
        ],
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transformIgnorePatterns": [
            "/output/.+\\.js"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts"
        ],
        "coverageReporters": [
            "json",
            "text",
            "clover",
            "html"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 35,
                "branch": 21,
                "functions": 30,
                "lines": 35
            }
        }
    }
}
